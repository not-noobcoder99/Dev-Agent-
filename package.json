{
  "name": "devagent-pro",
  "version": "1.0.0",
  "description": "An Autonomous AI Agent for Code Generation, Review, and Workflow Automation",
  "main": "index.js",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm start",
    "agent:generate": "ts-node agent/generate_code.ts",
    "agent:review": "ts-node agent/review_handler.ts",
