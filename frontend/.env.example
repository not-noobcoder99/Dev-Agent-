# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# ==============================================
# DATABASE CONFIGURATION
# ==============================================

# MongoDB Connection String
# Get this from MongoDB Atlas or use local: mongodb://localhost:27017/devagent
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/devagent

# ==============================================
# AUTHENTICATION CONFIGURATION
# ==============================================

# NextAuth.js Base URL (change in production)
NEXTAUTH_URL=http://localhost:3000

# NextAuth.js Secret (REQUIRED - Generate with: openssl rand -base64 32)
# Must be at least 32 characters long
NEXTAUTH_SECRET=

# ==============================================
# OAUTH PROVIDERS (OPTIONAL)
# ==============================================

# GitHub OAuth App
# Create at: https://github.com/settings/developers
# Callback URL: http://localhost:3000/api/auth/callback/github
GITHUB_ID=
GITHUB_SECRET=

# Google OAuth
# Create at: https://console.cloud.google.com/apis/credentials
# Callback URL: http://localhost:3000/api/auth/callback/google
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# ==============================================
# AI API KEYS
# ==============================================

# Together AI API Key (for code generation)
# Get from: https://api.together.xyz/
# This is a fallback - users can add their own keys in settings
TOGETHER_API_KEY=

# ==============================================
# OPTIONAL: OTHER API KEYS
# ==============================================

# CodeRabbit API (for code review)
CODERABBIT_API_KEY=

# Oumi API (for quality evaluation)
OUMI_API_KEY=

# Kestra API (for workflow orchestration)
KESTRA_API_URL=http://localhost:8080
KESTRA_API_KEY=
